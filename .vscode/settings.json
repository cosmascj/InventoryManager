{
    "eslint.validate": [
      "javascript",
      "javascriptreact",
      "typescript"
    ],
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
    "prettier.enable": false,
    "prettier.printWidth": 90,
    "prettier.requireConfig": true,
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "eslint.format.enable": true
  }